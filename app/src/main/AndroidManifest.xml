<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.destiny.gamegerbanglogika">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Mulai.ScoreActivity"
            android:theme="@style/AppThemeNoAction"></activity>
        <activity android:name=".Mulai.SoalActivity" />
        <activity android:name=".Mulai.StartActivity"
            android:label="Mulai"
            android:parentActivityName=".HomeActivity" />
        <activity
            android:name=".LatihanActivity"
            android:label="Latihan"
            android:parentActivityName=".HomeActivity" />
        <activity
            android:name=".HomeActivity"
            android:theme="@style/AppThemeNoAction" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppThemeNoAction">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>